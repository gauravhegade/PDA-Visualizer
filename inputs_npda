        },
        "a": {
            "Z": {("q0", ("A","Z"))},
            "B": {("q0", ("A","B"))},
        },
        "b": {
            "A": {("q0", ("B","A"))},
        },
    },
    "q1": {
        "": {
            "Z": {("qf","")},
        },
        "a": {
            "A": {("q1","")},
            "B": {("q1","")},
        },
    },
}
