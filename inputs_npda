{
    "q0": {
        "": {
            "#": {("q2", "#")},
        },
        "a": {
            "#": {("q0", ("A", "#"))},
            "A": {
                ("q0", ("A", "A")),
                ("q1", ""),
            },
            "B": {("q0", ("A", "B"))},
        },
        "b": {
            "#": {("q0", ("B", "#"))},
            "A": {("q0", ("B", "A"))},
            "B": {
                ("q0", ("B", "B")),
                ("q1", ""),
            },
        },
    },
    "q1": {
        "": {"#": {("q2", "#")}},
        "a": {"A": {("q1", "")}},
        "b": {"B": {("q1", "")}},
    },
},
